{
  "name": "finalroundapp-server",
  "version": "1.0.0",
  "description": "Backend server for FinalRoundApp AI meeting assistant app using Deepgram Nova 3 and OpenAI",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "type-check": "tsc --noEmit",
    "seed": "tsx src/seed.ts",
    "set-admin": "tsx src/set-admin.ts",
    "analyze-costs": "tsx src/scripts/analyze-plan-costs.ts"
  },
  "keywords": [
    "deepgram",
    "openai",
    "ai",
    "meeting",
    "assistant",
    "meeting-assistant",
    "real-time",
    "meeting-assistant-app",
    "undetectable",
    "screen capture",
    "speech-to-text",
    "websocket",
    "cross-platform",
    "windows",
    "linux",
    "macos",
    "ios",
    "android",
    "web",
    "browser",
    "chrome"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@deepgram/sdk": "^3.5.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^11.0.1",
    "mongodb": "^6.21.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.82.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.30",
    "@types/ws": "^8.18.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
